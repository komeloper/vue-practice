{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "api:build": "aspida --config aspida.config.cjs",
    "build": "nuxt build --dotenv .env.local",
    "dev": "nuxt dev --dotenv .env.local",
    "fix": "npm run prettier && npm run lint:scripts --fix && npm run lint:styles --fix",
    "generate": "nuxt generate --dotenv .env.local",
    "generate:production": "nuxt generate --dotenv .env.production",
    "prettier": "prettier .",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxi typecheck",
    "lint": "npm run typecheck && npm run lint:scripts && npm run lint:style",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint \"src/**/*.{css,scss,sass,vue}\" --ignore-path .gitignore"
  },
  "dependencies": {
    "nuxt": "^3.14.1592",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@aspida/axios": "^1.14.0",
    "@nuxt/eslint-config": "^0.7.4",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@pinia/nuxt": "^0.9.0",
    "aspida": "^1.14.0",
    "axios": "^1.7.9",
    "eslint": "^9.17.0",
    "eslint-plugin-prettier": "^5.2.1",
    "install": "^0.13.0",
    "msw": "^2.6.8",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "postcss-html": "^1.7.0",
    "prettier": "^3.4.2",
    "stylelint": "^16.12.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10"
  }
}
